 // Создание стека (где целые числа)  
    std::stack<int> a;  
    // Добавлляем элементы  в стек  
    a.push(5);  
    a.push(10);  
    a.push(15);  
return 0;
}



#include <iostream>  // для вывода
using namespace std;
int main(){
    // Создаём стек для целых чисел
    stack<int> a;
    
    // Добавляем элементы
    a.push(10);
    a.push(30);
    a.push(70);
